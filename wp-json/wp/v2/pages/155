3d\u0430\u0440\u043a\u043e\u043c\u0430\u043d\u043e\u0432 \u0437\u0434\u0435\u0441\u044c \u0441\u043d\u043e\u0432\u0430 \u0434\u0435\u043b\u0430\u044e\u0442 \u043b\u044e\u0434\u0435\u0439. \u042f \u043e\u0441\u0442\u0430\u043b\u0441\u044f \u0432 \u0441\u0442\u0430\u0446\u0438\u043e\u043d\u0430\u0440\u0435. \u041f\u043e \u043d\u0430\u0447\u0430\u043b\u0443 \u0431\u044b\u043b\u043e \u043e\u0447\u0435\u043d\u044c \u0442\u0440\u0443\u0434\u043d\u043e. \u041d\u043e, \u0441\u043e\u0432\u043c\u0435\u0441\u0442\u043d\u044b\u043c\u0438 \u0443\u0441\u0438\u043b\u0438\u044f\u043c\u0438 \u0443\u0434\u0430\u043b\u043e\u0441\u044c \u043f\u0435\u0440\u0435\u043b\u043e\u043c\u0438\u0442\u044c \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044e. \u0422\u0435\u043f\u0435\u0440\u044c \u044f \u0432\u0441\u0435\u043c \u0441\u0432\u043e\u0438\u043c \u0431\u044b\u0432\u0448\u0438\u043c \u00ab\u0434\u0440\u0443\u0437\u044c\u044f\u043c\u00bb \u044d\u0442\u043e\u0442 \u0440\u0435\u0430\u0431\u0438\u043b\u0438\u0442\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0439 \u0446\u0435\u043d\u0442\u0440.<\/p>\n<p style=\"text-align: right;\"><strong>\u0421\u0442\u0430\u043d\u0438\u0441\u043b\u0430\u0432, 28 \u043b\u0435\u0442<\/strong><\/p>\n<\/div><\/div>\n<div class=\"su-quote su-quote-style-default\"><div class=\"su-quote-inner su-u-clearfix su-u-trim\">\n<img decoding=\"async\" loading=\"lazy\" class=\"alignleft size-full wp-image-281\" src=\"http:\/\/europacenter.org.ua\/wp-content\/uploads\/2016\/10\/reviews-7.jpg\" alt=\"10\" width=\"120\" height=\"120\" \/>\u041f\u0438\u043b \u0432\u0441\u0435\u0433\u0434\u0430, \u043d\u043e \u0432 \u043c\u0435\u0440\u0443. \u0412\u043e \u0432\u0441\u044f\u043a\u043e\u043c \u0441\u043b\u0443\u0447\u0430\u0435, \u044f \u0442\u0430\u043a \u0441\u0430\u043c \u0434\u0443\u043c\u0430\u043b. \u0411\u0435\u0437 \u0441\u043f\u0438\u0440\u0442\u043d\u043e\u0433\u043e \u043d\u0435 \u043c\u043e\u0433 \u0441\u0435\u0441\u0442\u044c \u0437\u0430 \u0441\u0442\u043e\u043b, \u0432\u0441\u0442\u0440\u0435\u0442\u0438\u0442\u044c\u0441\u044f \u0441 \u0434\u0440\u0443\u0437\u044c\u044f\u043c\u0438. \u0421 \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u0435\u043c \u043e\u0442\u043c\u0435\u0447\u0430\u043b \u043b\u044e\u0431\u043e\u0439 \u043f\u0440\u0430\u0437\u0434\u043d\u0438\u043a. \u0418 \u043a\u0430\u043a-\u0442\u043e \u043d\u0435\u0437\u0430\u043c\u0435\u0442\u043d\u043e \u0441\u0442\u0430\u043b \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u043c \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u0438\u043a\u043e\u043c. \u0414\u0430\u0436\u0435 \u0438 \u043d\u0435 \u0441\u043a\u0430\u0436\u0443, \u0432 \u043a\u0430\u043a\u043e\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u0436\u0438\u0437\u043d\u0438 \u043f\u043e\u0442\u0435\u0440\u044f\u043b \u043f\u043e\u043b\u043d\u044b\u0439 \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u044c \u043d\u0430\u0434 \u0441\u043e\u0431\u043e\u0439. \u0415\u0441\u0442\u044c \u0431\u0443\u0442\u044b\u043b\u043a\u0430, \u0437\u043d\u0430\u0447\u0438\u0442 \u0436\u0438\u0437\u043d\u044c \u043d\u0430 \u0434\u0430\u043d\u043d\u044b\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u0443\u0434\u0430\u043b\u0430\u0441\u044c. \u0412\u043e\u0442 \u0438 \u0432\u0441\u0435 \u043c\u043e\u0438 \u0436\u0435\u043b\u0430\u043d\u0438\u044f \u0432 \u0442\u043e \u0432\u0440\u0435\u043c\u044f. \u041a\u0430\u043a-\u0442\u043e \u0432 \u0437\u0430\u043f\u043e\u0435 \u043c\u043d\u0435 \u0432\u044b\u0437\u0432\u0430\u043b\u0438 \u043d\u0430\u0440\u043a\u043e\u043b\u043e\u0433\u0430 \u0438 \u043e\u043d \u043f\u043e\u0441\u0442\u0430\u0432\u0438\u043b \u043c\u043d\u0435 \u043a\u0430\u043f\u0435\u043b\u044c\u043d\u0438\u0446\u0443. \u041b\u0435\u0436\u0443, \u0441\u043c\u043e\u0442\u0440\u044e, \u043a\u0430\u043a \u043a\u0430\u043f\u0430\u0435\u0442 \u043b\u0435\u043a\u0430\u0440\u0441\u0442\u0432\u043e \u0438 \u0434\u0443\u043c\u0430\u044e, \u0447\u0442\u043e \u0432\u043e\u0442 \u0442\u0430\u043a \u0438 \u043c\u043e\u044f \u0436\u0438\u0437\u043d\u044c \u0443\u0445\u043e\u0434\u0438\u0442 \u0438\u0437 \u043c\u0435\u043d\u044f, \u043f\u043e \u043a\u0430\u043f\u043b\u0435. \u0418 \u043a\u0430\u043a \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u0440\u043e\u0442\u0440\u0435\u0437\u0432\u0435\u043b, \u043f\u043e\u0448\u0451\u043b \u043a \u043d\u0430\u0440\u043a\u043e\u043b\u043e\u0433\u0443 \u0432 \u0440\u0435\u0430\u0431\u0438\u043b\u0438\u0442\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0439 \u0446\u0435\u043d\u0442\u0440 \u00ab\u0415\u0432\u0440\u043e\u043f\u0430\u00bb. \u0421\u043d\u0430\u0447\u0430\u043b\u0430 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0434\u043d\u0435\u0439 \u043c\u043d\u0435 \u0447\u0435\u0433\u043e-\u0442\u043e \u043a\u0430\u043f\u0430\u043b\u0438, \u0447\u0438\u0441\u0442\u0438\u043b\u0438 \u043a\u0440\u043e\u0432\u044c, \u0434\u0430\u0432\u0430\u043b\u0438 \u0441\u043d\u043e\u0442\u0432\u043e\u0440\u043d\u043e\u0435. \u041f\u043e\u0442\u043e\u043c, \u043a\u043e\u0433\u0434\u0430 \u044f \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u043f\u043e\u0447\u0443\u0432\u0441\u0442\u0432\u043e\u0432\u0430\u043b \u0441\u0435\u0431\u044f \u0431\u043e\u0434\u0440\u0435\u0435, \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0438\u043b\u0438 \u043e\u0441\u0442\u0430\u0442\u044c\u0441\u044f \u043d\u0430 \u0440\u0435\u0430\u0431\u0438\u043b\u0438\u0442\u0430\u0446\u0438\u043e\u043d\u043d\u044b\u0439 \u043a\u0443\u0440\u0441. \u041d\u0443, \u0434\u0443\u043c\u0430\u044e \u044f, \u0437\u0430 \u0441\u0442\u0435\u043d\u043e\u0439 \u0441\u0442\u0430\u0446\u0438\u043e\u043d\u0430\u0440\u0430 \u044f \u0442\u043e\u0447\u043d\u043e \u0441\u043e\u0440\u0432\u0443\u0441\u044c \u0438 \u0437\u0430\u043f\u044c\u044e, \u0443\u0436 \u043b\u0443\u0447\u0448\u0435 \u0437\u0434\u0435\u0441\u044c \u043e\u0441\u0442\u0430\u043d\u0443\u0441\u044c. \u0418 \u0442\u0430\u043a \u043e\u0441\u0442\u0430\u043b\u0441\u044f \u043d\u0430 \u0442\u0440\u0438 \u043c\u0435\u0441\u044f\u0446\u0430. \u0422\u0435\u043f\u0435\u0440\u044c \u043c\u043d\u0435 \u043a\u0430\u0436\u0435\u0442\u0441\u044f, \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u043e \u043e\u0441\u0442\u0430\u043b\u0441\u044f, \u0438 \u043e\u043a\u0430\u0437\u0430\u043b\u043e\u0441\u044c \u043a \u043b\u0443\u0447\u0448\u0435\u043c\u0443. \u0421 \u043d\u0430\u043c\u0438, \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u0438\u043a\u0430\u043c\u0438, \u0440\u0430\u0431\u043e\u0442\u0430\u043b\u0438 \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0435 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u044b. \u0421 \u043d\u0438\u043c\u0438 \u0431\u044b\u043b\u043e \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e \u043e\u0431\u0449\u0430\u0442\u044c\u0441\u044f \u043f\u043e \u043b\u044e\u0431\u044b\u043c \u0442\u0435\u043c\u0430\u043c. \u041c\u044b \u0438\u0445 \u0443\u0432\u0430\u0436\u0430\u043b\u0438 \u0438 \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0441\u043e\u0431\u043b\u044e\u0434\u0430\u043b\u0438 \u0432\u0441\u0435 \u043f\u0440\u0435\u0434\u043f\u0438\u0441\u0430\u043d\u0438\u0438. \u0422\u0435\u043f\u0435\u0440\u044c \u044f \u043d\u0435 \u043f\u044c\u044e. \u0421\u043e\u0432\u0441\u0435\u043c. \u0425\u043e\u0447\u0443 \u043f\u043e\u0441\u043e\u0432\u0435\u0442\u043e\u0432\u0430\u0442\u044c \u0432\u0441\u0435\u043c, \u043a\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u0431\u0440\u043e\u0441\u0438\u0442\u044c \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u044c, \u043e\u0431\u0440\u0430\u0449\u0430\u0439\u0442\u0435\u0441\u044c \u0432 \u00ab\u0415\u0432\u0440\u043e\u043f\u0443\u00bb. \u042d\u0442\u043e \u043f\u0440\u043e\u0432\u0435\u0440\u0435\u043d\u043d\u044b\u0439 \u0441\u043f\u043e\u0441\u043e\u0431.<\/p>\n<p style=\"text-align: right;\"><b>\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440<\/b><strong>, 34 \u0433\u043e\u0434\u0430<\/strong><\/p>\n<\/div><\/div>\n<div class=\"su-quote su-quote-style-default\"><div class=\"su-quote-inner su-u-clearfix su-u-trim\">\n<img decoding=\"async\" loading=\"lazy\" class=\"alignleft s